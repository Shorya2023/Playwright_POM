pipeline {
    agent any
    stages {
        stage('Pull Docker Image') {
            steps {
                sh 'docker pull node'
            }
        }
        stage('Run Docker Container') {
            steps {
                script {
                    // Run the Docker container
                    def dockerImage = docker.image('node')
                    dockerImage.run('-d -p 9091:8080')
                }
            }
        }
    }
}
